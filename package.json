{
  "name": "danishpraka.sh",
  "version": "1.0.0",
  "description": "Personal blog made with 11ty.",
  "main": "index.js",
  "scripts": {
    "build": "eleventy && npm run sass",
    "serve": "npm run clean && concurrently --success all 'eleventy --serve' 'npm run sass:watch'",
    "sass": "sass styles:_site/styles --no-source-map --style=compressed --update",
    "sass:watch": "npm run sass && npm run sass -- --watch",
    "clean": "rm -rf _site/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arpitbatra123/danishpraka.sh.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arpitbatra123/danishpraka.sh/issues"
  },
  "homepage": "https://github.com/arpitbatra123/danishpraka.sh#readme",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "concurrently": "^6.4.0",
    "eleventy-plugin-embed-twitter": "^1.3.3",
    "markdown-it-attrs": "^4.1.0",
    "markdown-it-image-lazy-loading": "^1.1.0",
    "sass": "^1.43.5"
  }
}
